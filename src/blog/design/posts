posts: [post]

post: {
  title: string,
  author: string,
  text:  markdownString,
  deletedByAdmin: bool,
  id: number,
  comments: [comment]
}

comment: {
  author: string,
  text: string,
  deletedByAdmin: bool,
  id: number,
  replys: [reply]
}

reply: {
  author: string,
  at: string,
  deletedByAdmin: bool,
  text: string,
  id: number
}